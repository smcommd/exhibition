{
  "name": "exhibition_archive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002 -H 0.0.0.0",
    "dev:lan": "IP=$(ipconfig getifaddr en0 || ipconfig getifaddr en1); echo \"Network URL: http://$IP:3002\"; next dev -p 3002 -H 0.0.0.0",
    "build": "next build",
    "generate:profiles": "node scripts/generate-profile-manifest.mjs",
    "generate:profiles:s3": "PROFILE_IMAGE_SOURCE=s3 node scripts/generate-profile-manifest.mjs",
    "start": "next start -p 3002 -H 0.0.0.0",
    "start:lan": "IP=$(ipconfig getifaddr en0 || ipconfig getifaddr en1); echo \"Network URL: http://$IP:3002\"; next start -p 3002 -H 0.0.0.0",
    "lint": "next lint"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.400.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/supabase-js": "^2.33.2",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "embla-carousel": "^8.6.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^10.16.4",
    "image-size": "^1.1.0",
    "mime-types": "^3.0.1",
    "next": "^13.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.5",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.5.2",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
